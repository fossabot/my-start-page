{
  "name": "My Start Page",
  "description": "Minimalist start page to use for new/home tab in browser",
  "author": "Michael Sprague",
  "short_name": "My Start Page",
  "icons": {
    "16": "./images/icon-16.png",
    "24": "./images/icon-24.png",
    "32": "./images/icon-32.png",
    "48": "./images/icon-48.png",
    "64": "./images/icon-64.png",
    "128": "./images/icon-128.png"
  },
  "version": "0.0.1",
  "manifest_version": 2,
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "cookies",
    "geolocation",
    "storage",
    "unlimitedStorage",
    "https://my-start-page.netlify.app/*"
  ]
}
